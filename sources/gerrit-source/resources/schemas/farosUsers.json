{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "_account_id": {
      "type": "integer"
    },
    "name": {
      "type": ["string", "null"]
    },
    "email": {
      "type": ["string", "null"]
    },
    "username": {
      "type": ["string", "null"]
    },
    "display_name": {
      "type": ["string", "null"]
    },
    "status": {
      "type": ["string", "null"]
    },
    "registered_on": {
      "type": ["string", "null"]
    },
    "inactive": {
      "type": ["boolean", "null"]
    },
    "tags": {
      "type": ["array", "null"],
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["_account_id"]
}