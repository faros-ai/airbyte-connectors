{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "parent": {
      "type": ["string", "null"]
    },
    "description": {
      "type": ["string", "null"]
    },
    "state": {
      "type": ["string", "null"]
    },
    "branches": {
      "type": ["object", "null"]
    },
    "labels": {
      "type": ["object", "null"]
    },
    "web_links": {
      "type": ["array", "null"],
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "image_url": {
            "type": ["string", "null"]
          }
        }
      }
    }
  },
  "required": ["id", "name"]
}