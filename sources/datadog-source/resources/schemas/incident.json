{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id"],
  "properties": {
    "attributes": {
      "type": "object",
      "required": ["title"],
      "properties": {
        "created": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the incident was created."
        },
        "detected": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the incident was detected."
        },
        "modified": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the incident was last modified."
        },
        "resolved": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the incident's state was set to resolved."
        },
        "title": {
          "type": "string",
          "description": "The title of the incident, which summarizes what happened."
        }
      }
    },
    "id": {
      "type": "string",
      "description": "The incident's ID."
    },
    "relationships": {
      "type": "object",
      "properties": {
        "commander_user": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            }
          }
        },
        "created_by_user": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            }
          }
        },
        "last_modified_by_user": {
          "type": "object",
          "properties": {
            "properties": {
              "id": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
