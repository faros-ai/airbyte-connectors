{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Datadog Spec",
    "type": "object",
    "required": [
      "apiKey",
      "applicationKey"
    ],
    "additionalProperties": false,
    "properties": {
      "apiKey": {
        "type": "string",
        "title": "DataDog API Key",
        "airbyte_secret": true
      },
      "applicationKey": {
        "type": "string",
        "title": "DataDog Application Key",
        "airbyte_secret": true
      },
      "pageSize": {
        "type": "number",
        "title": "Page size to be used when retrieving paginated data from DataDog"
      }
    }
  }
}
