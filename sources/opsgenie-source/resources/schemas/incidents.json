{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "impactedServices": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tinyId": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "createdAt": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    },
    "priority": {
      "type": "string"
    },
    "ownerTeam": {
      "type": "string"
    },
    "responders": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        }
      }
    },
    "extraProperties": {
      "type": "object"
    },
    "links": {
      "type": "object",
      "properties": {
        "web": {
          "type": "string"
        },
        "api": {
          "type": "string"
        }
      }
    },
    "impactStartDate": {
      "type": "string"
    },
    "actions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "timelines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "group": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "eventTime": {
            "type": "string"
          },
          "hidden": {
            "type": "boolean"
          },
          "actor": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            }
          },
          "title": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string"
              },
              "content": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
