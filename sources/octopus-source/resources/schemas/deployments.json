{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Changes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Version": {
            "type": "string"
          },
          "Commits": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "Id": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "ChangesMarkdown": {
      "type": "string"
    },
    "ChannelId": {
      "type": "string"
    },
    "Comments": {
      "type": "string"
    },
    "DeployedBy": {
      "type": "string"
    },
    "DeployedById": {
      "type": "string"
    },
    "DeployedToMachineIds": {
      "type": "array"
    },
    "DeploymentProcessId": {
      "type": "string"
    },
    "EnvironmentId": {
      "type": "string"
    },
    "ExcludedMachineIds": {
      "type": "array"
    },
    "FailureEncountered": {
      "type": "string"
    },
    "ForcePackageRedeployment": {
      "type": "boolean"
    },
    "Id": {
      "type": "string"
    },
    "Created": {
      "type": "string"
    },
    "_extra": {
      "SpaceName": {
        "type": "string"
      },
      "ProjectName": {
        "type": "string"
      },
      "EnvironmentName": {
        "type": "string"
      },
      "Task": {
        "type": "object",
        "properties": {
          "State": {
            "type": "string"
          },
          "ErrorMessage": {
            "type": "string"
          },
          "QueueTime": {
            "type": "string"
          },
          "StartTime": {
            "type": "string"
          },
          "CompletedTime": {
            "type": "string"
          }
        }
      },
      "Process": {
        "type": "object",
        "properties": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "Name": {
                "type": "string"
              },
              "Properties": {
                "type": "object"
              },
              "Actions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "Name": {
                      "type": "string"
                    },
                    "Properties": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
