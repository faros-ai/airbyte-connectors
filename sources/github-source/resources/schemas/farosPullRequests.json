{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "org": {
      "type": "string"
    },
    "repo": {
      "type": "string"
    },
    "additions": {
      "type": "integer"
    },
    "author": {
      "type": "object",
      "properties": {
        "login": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "html_url": {
          "type": "string"
        }
      }
    },
    "body": {
      "type": "string"
    },
    "changedFiles": {
      "type": "integer"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "commits": {
      "type": "object",
      "properties": {
        "totalCount": {
          "type": "integer"
        }
      }
    },
    "comments": {
      "type": "object",
      "properties": {
        "totalCount": {
          "type": "integer"
        }
      }
    },
    "deletions": {
      "type": "integer"
    },
    "isDraft": {
      "type": "boolean"
    },
    "number": {
      "type": "integer"
    },
    "mergedAt": {
      "type": "string",
      "format": "date-time"
    },
    "mergeCommit": {
      "type": "object",
      "properties": {
        "oid": {
          "type": "string"
        }
      }
    },
    "reviewEvents": {
      "type": "object",
      "properties": {
        "nodes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
      }
    },
    "files": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "path": {
            "type": "string"
          },
          "additions": {
            "type": "integer"
          },
          "deletions": {
            "type": "integer"
          }
        }
      }
    },
    "reviews": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "author": {
            "type": "object",
            "properties": {
              "login": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "html_url": {
                "type": "string"
              }
            }
          },
          "state": {
            "type": "string"
          },
          "submittedAt": {
            "type": "string",
            "format": "date-time"
          },
          "databaseId": {
            "type": "integer"
          },
          "url": {
            "type": "string"
          },
          "comments": {
            "type": "object",
            "properties": {
              "totalCount": {
                "type": "integer"
              }
            }
          }
        }
      }
    },
    "reviewRequests": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "requestedReviewer": {
            "oneOf": [
              {
                "type": "object",
                "properties": {
                  "login": {
                    "type": "string"
                  },
                  "html_url": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "members": {
                    "type": "object",
                    "properties": {
                      "nodes": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "login": {
                              "type": "string"
                            },
                            "name": {
                              "type": "string"
                            },
                            "type": {
                              "type": "string"
                            },
                            "html_url": {
                              "type": "string"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      }
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        }
      }
    },
    "state": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    },
    "url": {
      "type": "string"
    },
    "baseRefName": {
      "type": "string"
    },
    "baseRepository": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "owner": {
          "type": "object",
          "properties": {
            "login": {
              "type": "string"
            }
          }
        }
      }
    },
    "headRefName": {
      "type": "string"
    },
    "headRepository": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "owner": {
          "type": "object",
          "properties": {
            "login": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
