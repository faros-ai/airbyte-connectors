{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repository": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "body": {
      "type": "string"
    },
    "draft": {
      "type": "boolean"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "published_at": {
      "type": "string",
      "format": "date-time"
    },
    "author": {
      "type": "object",
      "properties": {
        "login": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "html_url": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "tag_name": {
      "type": "string"
    }
  }
}
