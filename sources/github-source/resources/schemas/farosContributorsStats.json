{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "org": {
      "type": "string"
    },
    "repo": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "properties": {
        "login": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "html_url": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "total": {
      "type": "number"
    },
    "weeks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "w": {
            "type": "number"
          },
          "a": {
            "type": "number"
          },
          "d": {
            "type": "number"
          },
          "c": {
            "type": "number"
          }
        }
      }
    }
  }
}
