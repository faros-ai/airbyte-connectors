{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repository": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "commit": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        },
        "committedDate": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  }
}
