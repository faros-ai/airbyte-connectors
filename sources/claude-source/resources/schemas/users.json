{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "name": {
      "type": ["string", "null"]
    },
    "role": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "added_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
