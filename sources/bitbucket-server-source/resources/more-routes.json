{
  "projects": {
    "getProject": {
      "headers": {
        "accept": "application/json"
      },
      "method": "GET",
      "params": {
        "projectKey": {
          "in": "path",
          "required": true,
          "type": "string"
        }
      },
      "url": "/rest/api/latest/projects/{projectKey}"
    },
    "getRepositories": {
      "headers": {
        "accept": "application/json"
      },
      "method": "GET",
      "params": {
        "projectKey": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "start": {
          "in": "query",
          "type": "integer"
        },
        "limit": {
          "in": "query",
          "type": "integer"
        }
      },
      "url": "/rest/api/latest/projects/{projectKey}/repos"
    }
  },
  "pullRequests": {
    "getDiff": {
      "headers": {
        "accept": "text/plain;charset=utf-8"
      },
      "method": "GET",
      "params": {
        "projectKey": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "repositorySlug": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "pullRequestId": {
          "in": "path",
          "required": true,
          "type": "integer"
        }
      },
      "url": "/rest/api/latest/projects/{projectKey}/repos/{repositorySlug}/pull-requests/{pullRequestId}.diff"
    }
  },
  "repos": {
    "getCommits": {
      "headers": {
        "accept": "application/json"
      },
      "method": "GET",
      "params": {
        "merges": {
          "in": "query",
          "type": "string"
        },
        "projectKey": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "repositorySlug": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "since": {
          "in": "query",
          "type": "string"
        },
        "start": {
          "in": "query",
          "type": "integer"
        },
        "limit": {
          "in": "query",
          "type": "integer"
        }
      },
      "url": "/rest/api/latest/projects/{projectKey}/repos/{repositorySlug}/commits"
    },
    "getTags": {
      "headers": {
        "accept": "application/json"
      },
      "method": "GET",
      "params": {
        "projectKey": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "repositorySlug": {
          "in": "path",
          "required": true,
          "type": "string"
        },
        "orderBy": {
          "in": "query",
          "type": "string"
        },
        "start": {
          "in": "query",
          "type": "integer"
        },
        "limit": {
          "in": "query",
          "type": "integer"
        }
      },
      "url": "/rest/api/latest/projects/{projectKey}/repos/{repositorySlug}/tags"
    }
  }
}
