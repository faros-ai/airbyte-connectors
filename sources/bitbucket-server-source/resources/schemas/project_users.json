{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "properties": {
        "displayName": {
          "type": "string"
        },
        "links": {
          "type": "object",
          "properties": {
            "htmlUrl": {
              "type": "string",
              "format": "uri"
            }
          }
        },
        "type": {
          "type": "string"
        },
        "nickname": {
          "type": "string"
        },
        "accountId": {
          "type": "string",
          "format": "uuid"
        }
      }
    },
    "project": {
      "type": "object",
      "properties": {
        "slug": {
          "type": "string"
        }
      }
    }
  }
}
