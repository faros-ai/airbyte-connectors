{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "files": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "deletions": {
            "type": "integer"
          },
          "additions": {
            "type": "integer"
          },
          "from": {
            "type": "string"
          },
          "to": {
            "type": "string"
          },
          "new": {
            "type": "boolean"
          },
          "deleted": {
            "type": "boolean"
          }
        }
      }
    },
    "computedProperties": {
      "type": "object",
      "properties": {
        "pullRequest": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "repository": {
              "type": "object",
              "properties": {
                "fullName": {
                  "type": "string"
                }
              }
            },
            "updatedDate": {
              "type": "integer"
            }
          }
        }
      }
    }
  }
}
