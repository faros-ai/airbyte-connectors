          "type": "string"
        },
        "emailAddress": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "displayName": {
          "type": "string"
        },
        "active": {
          "type": "boolean"
        },
        "slug": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "avatarUrl": {
          "type": "string"
        },
        "links": {
          "type": "object",
          "properties": {
            "self": {
              "type": "array",
