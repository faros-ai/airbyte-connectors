                "properties": {
                  "href": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "committerTimestamp": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "parents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
