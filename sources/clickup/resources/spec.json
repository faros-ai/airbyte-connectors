{
  "documentationUrl": "https://clickup.com/api",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Click Up connector",
    "type": "object",
    "required": [
      "personal_token",
      "space_number"
    ],
    "additionalProperties": false,
    "properties": {
      "personal_token": {
        "type": "string",
        "title": "Click Up personal token",
        "airbyte_secret": true
      },
      "space_number": {
        "type": "string",
        "title": "clickup space number",
        "airbyte_secret": true
      }
    }
  }
}