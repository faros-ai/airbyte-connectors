{
  "streams": [
    {
      "stream": {
        "name": "builds",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "_class": {
              "type": "string"
            },
            "actions": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "_class": {
                    "type": "string"
                  },
                  "lastBuiltRevision": {
                    "type": "object",
                    "properties": {
                      "SHA1": {
                        "type": "string"
                      },
                      "remoteUrls": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            },
            "building": {
              "type": "boolean"
            },
            "displayName": {
              "type": "string"
            },
            "duration": {
              "type": "number"
            },
            "id": {
              "type": "string"
            },
            "number": {
              "type": "number"
            },
            "result": {
              "type": "string"
            },
            "timestamp": {
              "type": "number"
            },
            "url": {
              "type": "string"
            },
            "fullDisplayName": {
              "type": "string"
            }
          }
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "source_defined_cursor": true,
        "source_defined_primary_key": [["fullDisplayName"]]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append",
      "cursor_field": ["number"]
    }
  ]
}
