{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "_class": {
      "type": "string"
    },
    "actions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_class": {
            "type": "string"
          },
          "lastBuiltRevision": {
            "type": "object",
            "properties": {
              "SHA1": {
                "type": "string"
              },
              "remoteUrls": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "building": {
      "type": "boolean"
    },
    "displayName": {
      "type": "string"
    },
    "duration": {
      "type": "number"
    },
    "fullDisplayName": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "number": {
      "type": "number"
    },
    "result": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "url": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  },
  "required": [
    "_class",
    "building",
    "displayName",
    "duration",
    "fullDisplayName",
    "id",
    "number",
    "result",
    "timestamp",
    "url",
    "description"
  ]
}
