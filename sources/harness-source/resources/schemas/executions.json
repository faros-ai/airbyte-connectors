{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "planExecutionId": {
      "type": "string"
    },
    "pipelineIdentifier": {
      "type": "string"
    },
    "orgIdentifier": {
      "type": "string"
    },
    "projectIdentifier": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "startTs": {
      "type": "number"
    },
    "endTs": {
      "type": ["number", "null"]
    },
    "runSequence": {
      "type": ["number", "null"]
    },
    "successfulStagesCount": {
      "type": ["number", "null"]
    },
    "failedStagesCount": {
      "type": ["number", "null"]
    },
    "runningStagesCount": {
      "type": ["number", "null"]
    },
    "totalStagesCount": {
      "type": ["number", "null"]
    },
    "modules": {
      "type": ["array", "null"],
      "items": {
        "type": "string"
      }
    },
    "executorInfo": {
      "type": ["object", "null"],
      "properties": {
        "triggerType": {
          "type": "string"
        },
        "username": {
          "type": ["string", "null"]
        },
        "email": {
          "type": ["string", "null"]
        }
      }
    },
    "executionTriggerInfo": {
      "type": ["object", "null"],
      "properties": {
        "triggerType": {
          "type": "string"
        },
        "triggeredBy": {
          "type": ["object", "null"],
          "properties": {
            "identifier": {
              "type": "string"
            },
            "extraInfo": {
              "type": ["object", "null"]
            }
          }
        }
      }
    },
    "moduleInfo": {
      "type": ["object", "null"],
      "properties": {
        "cd": {
          "type": ["object", "null"],
          "properties": {
            "serviceIdentifiers": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "envIdentifiers": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "serviceDefinitionTypes": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "environmentTypes": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "infrastructureTypes": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "infrastructureIdentifiers": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "infrastructureNames": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            },
            "artifactDisplayNames": {
              "type": ["array", "null"],
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
