{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "key": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "includeInSnippetByDefault": {
      "type": "boolean"
    },
    "defaultClientSideAvailability": {
      "type": "object",
      "properties": {
        "usingEnvironmentId": {
          "type": "boolean"
        },
        "usingMobileKey": {
          "type": "boolean"
        }
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "_links": {
      "type": "object"
    },
    "_id": {
      "type": "string"
    }
  }
}
