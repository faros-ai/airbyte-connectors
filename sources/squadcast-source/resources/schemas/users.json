{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "contact": {
      "type": "object",
      "properties": {
        "dial_code": {
          "type": "string"
        },
        "phone_number": {
          "type": "string"
        }
      }
    },
    "secondary_emails": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "email_verified": {
      "type": "boolean"
    },
    "phone_verified": {
      "type": "boolean"
    },
    "in_grace_period": {
      "type": "boolean"
    },
    "time_zone": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "bio": {
      "type": "string"
    },
    "user_image": {
      "type": "boolean"
    },
    "role_id": {
      "type": "string"
    },
    "role": {
      "type": "string"
    }
  }
}
