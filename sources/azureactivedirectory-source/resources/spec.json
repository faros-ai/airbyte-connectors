{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Azure Active Directory Spec",
    "type": "object",
    "required": ["client_id", "client_secret", "tenant_id"],
    "additionalProperties": true,
    "properties": {
      "client_id": {
        "type": "string",
        "title": "clientID",
        "description": "Azure Active Directory Client ID",
        "airbyte_secret": true
      },
      "client_secret": {
        "type": "string",
        "title": "client secret",
        "description": "Azure Active Directory Client Secret",
        "airbyte_secret": true
      },
      "tenant_id": {
        "type": "string",
        "title": "tenantID",
        "description": "Azure Active Directory TenantID",
        "airbyte_secret": true
      },
      "auth_version": {
        "type": "string",
        "title": "auth version",
        "description": "Azure Active Directory Auth Version",
        "default": "v2.0"
      },
      "version": {
        "type": "string",
        "title": "graph version",
        "description": "Azure Active Directory Graph Version",
        "default": "v1.0"
      }
    }
  }
}
