{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Backlog Spec",
    "type": "object",
    "required": [
      "apiKey",
      "space" 
    ],
    "additionalProperties": false,
    "properties": {
      "space": {
        "type": "string",
        "title": "Each account have some spaces",
        "examples": [
          "devcube"
        ]
      }, 
      "apiKey": {
        "type": "string",
        "title": "Backlog Apikey",
        "airbyte_secret": true
      },
      "version": {
        "type": "string",
        "title": "version",
        "default":"v2" 
      }
    }
  }
}
