{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "projectKey": {
      "type": "string"
    },
    "chartEnabled": {
      "type": "boolean"
    },
    "subtaskingEnabled": {
      "type": "boolean"
    },
    "projectLeaderCanEditProjectLeader": {
      "type": "boolean"
    },
    "useWikiTreeView": {
      "type": "boolean"
    },
    "textFormattingRule": {
      "type": "string"
    },
    "displayOrder": {
      "type": "number"
    },
    "useDevAttributes": {
      "type": "boolean"
    },
    "versionMilestones": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "displayOrder": {
            "type": "number"
          },
          "projectId": {
            "type": "number"
          },
          "description": {
            "type": ["string", "null"]
          },
          "startDate": {
            "type": ["string", "null"]
          },
          "releaseDueDate": {
            "type": ["string", "null"]
          },
          "archived": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
