{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "boardId": {
      "type": "string"
    },
    "projectKey": {
      "type": "string"
    },
    "closedAt": {
      "type": "string",
      "format": "date-time"
    },
    "completedPoints": {
      "type": "number"
    },
    "completedInAnotherSprintPoints": {
      "type": "number"
    },
    "notCompletedPoints": {
      "type": "number"
    },
    "puntedPoints": {
      "type": "number"
    },
    "plannedPoints": {
      "type": "number"
    }
  }
}
