{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "name": {
      "type": "string",
      "description": "User's display name"
    },
    "email": {
      "type": "string",
      "description": "User's email address"
    },
    "lastUpdateTime": {
      "type": "string",
      "description": "Timestamp of last activity"
    },
    "activeDays": {
      "type": "number",
      "description": "Total active days in timeframe"
    },
    "disableCodeium": {
      "type": "boolean",
      "description": "Indicates if Windsurf access is disabled"
    },
    "lastAutocompleteUsageTime": {
      "type": "string",
      "description": "Most recent Tab/Autocomplete usage"
    },
    "lastChatUsageTime": {
      "type": "string",
      "description": "Most recent Cascade modality usage"
    },
    "lastCommandUsageTime": {
      "type": "string",
      "description": "Most recent command modality usage"
    },
    "usageTimestamps": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "All unique usage timestamps collected from analytics sources (ISO 8601 format)"
    },
    "minUsageTimestamp": {
      "type": "string",
      "description": "Minimum usage timestamp computed from all analytics sources (ISO 8601 format)"
    }
  },
  "required": [
    "name",
    "email",
    "lastUpdateTime",
    "activeDays",
    "lastAutocompleteUsageTime",
    "lastChatUsageTime",
    "lastCommandUsageTime",
    "usageTimestamps"
  ]
}
