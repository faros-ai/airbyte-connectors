{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "date": {
      "type": "string",
      "description": "Date in YYYY-MM-DD format"
    },
    "percent_code_written": {
      "type": ["number", "null"],
      "description": "Percentage of code written by AI (codeium_bytes / (codeium_bytes + user_bytes))"
    },
    "codeium_bytes": {
      "type": ["number", "null"],
      "description": "Total Codeium-generated bytes"
    },
    "user_bytes": {
      "type": ["number", "null"],
      "description": "Total user-written bytes"
    },
    "total_bytes": {
      "type": ["number", "null"],
      "description": "Total bytes (codeium_bytes + user_bytes)"
    },
    "codeium_bytes_by_autocomplete": {
      "type": ["number", "null"],
      "description": "Codeium bytes from autocomplete"
    },
    "codeium_bytes_by_command": {
      "type": ["number", "null"],
      "description": "Codeium bytes from command"
    }
  },
  "required": [
    "date"
  ]
}
