{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Windsurf Spec",
    "type": "object",
    "required": [
      "service_key"
    ],
    "additionalProperties": true,
    "properties": {
      "service_key": {
        "order": 0,
        "title": "Service Key",
        "type": "string",
        "description": "Your Windsurf service key with 'Teams Read-only' permissions. Keep this key secure and never expose it in client-side code or public repositories.",
        "airbyte_secret": true
      },
      "windsurf_api_url": {
        "order": 1,
        "type": "string",
        "title": "Windsurf API URL",
        "description": "The Windsurf API URL.",
        "default": "https://server.codeium.com/api/v1"
      },
      "timeout": {
        "order": 2,
        "type": "integer",
        "title": "Request Timeout",
        "description": "Timeout in milliseconds for each request to the Windsurf API",
        "default": 60000
      }
    }
  }
}
