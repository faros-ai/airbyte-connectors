{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "teamUid",
    "memberUid",
    "startedAt"
  ],
  "properties": {
    "teamUid": {
      "type": "string"
    },
    "memberUid": {
      "type": "string"
    },
    "startedAt": {
      "type": "string",
      "format": "date-time"
    },
    "endedAt": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    }
  }
}
