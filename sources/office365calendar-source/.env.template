# Office 365 Calendar Integration Test Configuration Template
# Copy this to .env and fill in your actual Azure AD application credentials

# Integration test variables (for integration-tests/)
INTEGRATION_TENANT_ID=your-azure-tenant-id-here
INTEGRATION_CLIENT_ID=your-azure-client-id-here
INTEGRATION_CLIENT_SECRET=your-azure-client-secret-here

# Real-world test variables (for real-world-tests/)
O365_TENANT_ID=your-azure-tenant-id-here
O365_CLIENT_ID=your-azure-client-id-here
O365_CLIENT_SECRET=your-azure-client-secret-here

# Optional test configuration
O365_TEST_TIMEOUT=30000
O365_TEST_MAX_EVENTS=100
O365_TEST_CUTOFF_DAYS=30
O365_DOMAIN_WIDE_DELEGATION=true