{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["incidentNumber", "startTime", "transitions"],
  "properties": {
    "alertCount": {
      "type": "number"
    },
    "currentPhase": {
      "type": "string"
    },
    "entityDisplayName": {
      "type": "string"
    },
    "entityId": {
      "type": "string"
    },
    "entityState": {
      "type": "string"
    },
    "entityType": {
      "type": "string"
    },
    "host": {
      "type": "string"
    },
    "incidentNumber": {
      "type": "string"
    },
    "lastAlertId": {
      "type": "string"
    },
    "lastAlertTime": {
      "type": "string"
    },
    "service": {
      "type": "string"
    },
    "startTime": {
      "type": "string"
    },
    "pagedTeams": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "pagedUsers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "pagedPolicies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "policy": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "slug": {
                "type": "string"
              }
            }
          },
          "team": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "slug": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "transitions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "at": {
            "type": "string"
          },
          "by": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "monitorName": {
      "type": "string"
    },
    "monitorType": {
      "type": "string"
    },
    "incidentLink": {
      "type": "string"
    }
  }
}
