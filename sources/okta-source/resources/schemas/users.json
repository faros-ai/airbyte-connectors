{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "created": {
      "type": "string"
    },
    "activated": {
      "type": ["string", "null"]
    },
    "statusChanged": {
      "type": ["string", "null"]
    },
    "lastLogin": {
      "type": ["string", "null"]
    },
    "lastUpdated": {
      "type": "string"
    },
    "passwordChanged": {
      "type": ["string", "null"]
    },
    "type": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        }
      }
    },
    "profile": {
      "type": "object",
      "properties": {
        "profileUrl": {
          "type": ["string", "null"]
        },
        "lastName": {
          "type": "string"
        },
        "zipCode": {
          "type": ["string", "null"]
        },
        "preferredLanguage": {
          "type": ["string", "null"]
        },
        "manager": {
          "type": ["string", "null"]
        },
        "city": {
          "type": ["string", "null"]
        },
        "displayName": {
          "type": ["string", "null"]
        },
        "nickName": {
          "type": ["string", "null"]
        },
        "secondEmail": {
          "type": ["string", "null"]
        },
        "honorificPrefix": {
          "type": ["string", "null"]
        },
        "title": {
          "type": ["string", "null"]
        },
        "locale": {
          "type": ["string", "null"]
        },
        "login": {
          "type": ["string", "null"]
        },
        "honorificSuffix": {
          "type": ["string", "null"]
        },
        "firstName": {
          "type": "string"
        },
        "primaryPhone": {
          "type": ["string", "null"]
        },
        "postalAddress": {
          "type": ["string", "null"]
        },
        "mobilePhone": {
          "type": ["string", "null"]
        },
        "streetAddress": {
          "type": ["string", "null"]
        },
        "countryCode": {
          "type": ["string", "null"]
        },
        "middleName": {
          "type": ["string", "null"]
        },
        "state": {
          "type": ["string", "null"]
        },
        "department": {
          "type": ["string", "null"]
        },
        "email": {
          "type": ["string", "null"]
        },
        "userType": {
          "type": ["string", "null"]
        }
      }
    },
    "credentials": {
      "type": "object",
      "properties": {
        "password": {
          "type": ["object", "null"]
        },
        "emails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            }
          }
        },
        "provider": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "name": {
              "type": "string"
            }
          }
        }
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
