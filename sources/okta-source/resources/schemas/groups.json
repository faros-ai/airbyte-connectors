{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "created": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string"
    },
    "lastMembershipUpdated": {
      "type": "string"
    },
    "objectClass": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "type": {
      "type": "string"
    },
    "profile": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": ["string", "null"]
        }
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "logo": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "href": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            }
          }
        },
        "users": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "apps": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        }
      }
    },
    "usersOfGroup": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        }
      }
    }
  }
}
