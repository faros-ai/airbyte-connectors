{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nextPageToken": {
      "type": "string"
    },
    "primaryTime": {
      "type": "string"
    },
    "timeStamp": {
      "type": "object",
      "properties": {
        "time": {
          "type": "string"
        }
      }
    },
    "actors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "user": {
            "type": "object",
            "properties": {
              "knownUser": {
                "type": "object",
                "properties": {
                  "personName": {
                    "type": "string"
                  },
                  "isCurrentUser": {
                    "type": "boolean"
                  }
                }
              }
            }
          },
          "impersonation": {
            "type": "object",
            "properties": {
              "impersonatedUser": {
                "type": "object",
                "properties": {
                  "knownUser": {
                    "type": "object",
                    "properties": {
                      "personName": {
                        "type": "string"
                      },
                      "isCurrentUser": {
                        "type": "boolean"
                      }
                    }
                  }
                }
              }
            }
          },
          "administrator": {
            "type": "object",
            "properties": {
              "admin": {
                "type": "string"
              }
            }
          },
          "anonymous": {
            "type": "object"
          },
          "system": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "targets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "driveItem": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "file": {
                "type": "object"
              },
              "folder": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  }
                }
              },
              "owner": {
                "type": "object",
                "properties": {
                  "user": {
                    "type": "object",
                    "properties": {
                      "knownUser": {
                        "type": "object",
                        "properties": {
                          "personName": {
                            "type": "string"
                          },
                          "isCurrentUser": {
                            "type": "boolean"
                          }
                        }
                      }
                    }
                  },
                  "domain": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "drive": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "root": {
                "type": "object"
              }
            }
          },
          "fileComment": {
            "type": "object",
            "properties": {
              "parent": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "title": {
                    "type": "string"
                  },
                  "driveItem": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    },
    "actions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "actor": {
            "type": "object",
            "properties": {
              "user": {
                "type": "object"
              }
            }
          },
          "target": {
            "type": "object",
            "properties": {
              "driveItem": {
                "type": "object"
              }
            }
          },
          "detail": {
            "type": "object",
            "properties": {
              "create": {
                "type": "object",
                "properties": {
                  "new": {
                    "type": "object"
                  }
                }
              },
              "edit": {
                "type": "object"
              },
              "move": {
                "type": "object",
                "properties": {
                  "addedParents": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "driveItem": {
                          "type": "object"
                        }
                      }
                    }
                  },
                  "removedParents": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "driveItem": {
                          "type": "object"
                        }
                      }
                    }
                  }
                }
              },
              "rename": {
                "type": "object",
                "properties": {
                  "oldTitle": {
                    "type": "string"
                  },
                  "newTitle": {
                    "type": "string"
                  }
                }
              },
              "delete": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  }
                }
              },
              "restore": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  }
                }
              },
              "permissionChange": {
                "type": "object",
                "properties": {
                  "addedPermissions": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string"
                        },
                        "user": {
                          "type": "object"
                        },
                        "group": {
                          "type": "object"
                        },
                        "domain": {
                          "type": "object"
                        },
                        "anyone": {
                          "type": "object"
                        }
                      }
                    }
                  },
                  "removedPermissions": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string"
                        },
                        "user": {
                          "type": "object"
                        },
                        "group": {
                          "type": "object"
                        },
                        "domain": {
                          "type": "object"
                        },
                        "anyone": {
                          "type": "object"
                        }
                      }
                    }
                  }
                }
              },
              "comment": {
                "type": "object",
                "properties": {
                  "post": {
                    "type": "object",
                    "properties": {
                      "subtype": {
                        "type": "string"
                      }
                    }
                  },
                  "assignment": {
                    "type": "object",
                    "properties": {
                      "subtype": {
                        "type": "string"
                      }
                    }
                  },
                  "suggestion": {
                    "type": "object"
                  }
                }
              },
              "dlpChange": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "info": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string"
                      }
                    }
                  }
                }
              },
              "reference": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  }
                }
              },
              "settingsChange": {
                "type": "object",
                "properties": {
                  "restrictionChanges": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "feature": {
                          "type": "string"
                        },
                        "newRestriction": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
