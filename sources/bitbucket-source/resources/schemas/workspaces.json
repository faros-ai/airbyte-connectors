{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "links": {
      "type": "object",
      "properties": {
        "ownersUrl": {
          "type": "string",
          "format": "uri"
        },
        "repositoriesUrl": {
          "type": "string",
          "format": "uri"
        },
        "htmlUrl": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "createdOn": {
      "type": "string",
      "format": "date-time"
    },
    "type": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "isPrivate": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    }
  }
}
