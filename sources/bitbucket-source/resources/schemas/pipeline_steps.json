{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "completedOn": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "uuid": {
      "type": "string"
    },
    "startedOn": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "type": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "runNumber": {
      "type": "number"
    },
    "maxTime": {
      "type": ["number", "null"]
    },
    "buildSecondsUsed": {
      "type": "number"
    },
    "durationInSeconds": {
      "type": ["number", "null"]
    },
    "pipeline": {
      "type": "object",
      "properties": {
        "uuid": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "image": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        }
      }
    },
    "scriptCommands": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "commandType": {
            "type": "string"
          },
          "command": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      }
    },
    "state": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "result": {
          "type": "object",
          "properties": {
            "name": {
              "type": ["string", "null"]
            },
            "type": {
              "type": ["string", "null"]
            }
          }
        }
      }
    },
    "trigger": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        }
      }
    },
    "teardownCommands": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "commandType": {
            "type": "string"
          },
          "command": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "action": {
            "type": "string"
          }
        }
      }
    },
    "setupCommands": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "commandType": {
            "type": "string"
          },
          "command": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  }
}
