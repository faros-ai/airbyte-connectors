{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Cursor Spec",
    "type": "object",
    "required": [
      "api_key"
    ],
    "additionalProperties": true,
    "properties": {
      "api_key": {
        "order": 0,
        "title": "API Key",
        "type": "string",
        "description": "The Cursor API key to use to access the API.",
        "airbyte_secret": true
      },
      "api_url": {
        "order": 1,
        "type": "string",
        "title": "API URL",
        "description": "The Cursor API URL.",
        "default": "https://api.cursor.com"
      },
      "cutoff_days": {
        "order": 2,
        "type": "integer",
        "title": "Cutoff Days",
        "default": 90,
        "description": "Only fetch data updated after cutoff"
      },
      "start_date": {
        "order": 3,
        "type": "string",
        "title": "Start Date",
        "description": "The date from which to start syncing data."
      },
      "end_date": {
        "order": 4,
        "type": "string",
        "title": "End Date",
        "description": "The date at which to stop syncing data."
      }
    }
  }
}
