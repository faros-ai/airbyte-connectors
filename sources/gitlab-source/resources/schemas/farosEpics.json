{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "group_id": {
      "type": "string"
    },
    "author_username": {
      "type": ["string", "null"]
    },
    "id": {
      "type": "integer"
    },
    "iid": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "state": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "web_url": {
      "type": "string"
    }
  },
  "required": ["group_id", "id", "title", "state", "created_at", "updated_at"]
}
