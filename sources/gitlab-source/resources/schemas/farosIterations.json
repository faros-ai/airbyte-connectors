{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "group_id": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "iid": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "state": {
      "type": "integer",
      "description": "1=upcoming, 2=current, 3=closed"
    },
    "start_date": {
      "type": "string",
      "format": "date"
    },
    "due_date": {
      "type": "string",
      "format": "date"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["group_id", "id", "iid", "title", "state", "start_date", "due_date", "updated_at"]
}
