{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "id",
    "group_id",
    "archived",
    "created_at",
    "default_branch",
    "description",
    "empty_repo",
    "name",
    "namespace",
    "owner",
    "path",
    "path_with_namespace",
    "updated_at",
    "visibility",
    "web_url"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "group_id": {
      "type": "string"
    },
    "syncRepoData": {
      "type": "boolean"
    },
    "archived": {
      "type": "boolean"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "default_branch": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "empty_repo": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "namespace": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "kind": {
          "type": "string"
        },
        "full_path": {
          "type": "string"
        }
      }
    },
    "owner": {
      "type": ["object", "null"]
    },
    "path": {
      "type": "string"
    },
    "path_with_namespace": {
      "type": "string"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "visibility": {
      "type": "string"
    },
    "web_url": {
      "type": "string"
    }
  }
}
