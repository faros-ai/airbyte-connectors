{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "group_id": {
      "type": "string"
    },
    "project_path": {
      "type": "string"
    },
    "author_username": {
      "type": "string"
    },
    "assignee_usernames": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "id": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "state": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["group_id", "project_path", "author_username", "assignee_usernames", "id", "title", "description", "state", "created_at", "updated_at", "labels"]
}