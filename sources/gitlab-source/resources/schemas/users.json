{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "group_path": {
      "type": "string"
    },
    "avatar_url": {
      "type": ["string", "null"]
    },
    "web_url": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "email": {
      "type": ["string", "null"]
    }
  }
}
