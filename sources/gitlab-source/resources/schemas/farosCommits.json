{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "org": {
      "type": "string"
    },
    "repo": {
      "type": "string"
    },
    "branch": {
      "type": "string"
    },
    "oid": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "authoredDate": {
      "type": "string",
      "format": "date-time"
    },
    "author": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      }
    },
    "committer": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      }
    },
    "additions": {
      "type": "number"
    },
    "deletions": {
      "type": "number"
    },
    "changedFilesIfAvailable": {
      "type": "number"
    },
    "group_id": {
      "type": "string"
    },
    "project_id": {
      "type": "string"
    }
  }
}
