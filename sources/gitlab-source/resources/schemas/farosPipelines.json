{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "integer"
    },
    "iid": {
      "type": "integer"
    },
    "project_id": {
      "type": "string"
    },
    "group_id": {
      "type": "string"
    },
    "project_path": {
      "type": "string"
    },
    "sha": {
      "type": "string"
    },
    "ref": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "started_at": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "finished_at": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "duration": {
      "type": ["number", "null"]
    },
    "web_url": {
      "type": "string"
    },
    "user": {
      "type": ["object", "null"]
    },
    "tag": {
      "type": "boolean"
    }
  }
}
