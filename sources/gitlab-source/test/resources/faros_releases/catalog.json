{
  "streams": [
    {
      "stream": {
        "name": "faros_releases",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "group_id": {
              "type": "string"
            },
            "project_path": {
              "type": "string"
            },
            "tag_name": {
              "type": "string"
            },
            "name": {
              "type": ["string", "null"]
            },
            "description": {
              "type": ["string", "null"]
            },
            "created_at": {
              "type": "string"
            },
            "released_at": {
              "type": ["string", "null"]
            },
            "_links": {
              "type": "object",
              "properties": {
                "self": {"type": "string"}
              }
            }
          },
          "required": ["group_id", "project_path", "tag_name", "created_at"]
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "source_defined_cursor": true,
        "default_cursor_field": ["created_at"],
        "source_defined_primary_key": [
          ["project_path"],
          ["tag_name"]
        ]
      },
      "sync_mode": "incremental",
      "cursor_field": ["created_at"],
      "destination_sync_mode": "append_dedup",
      "primary_key": [
        ["project_path"],
        ["tag_name"]
      ]
    }
  ]
}