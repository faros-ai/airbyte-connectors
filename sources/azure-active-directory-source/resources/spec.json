{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Azure Active Directory Spec",
    "type": "object",
    "required": [
      "token",
      "domain_name"
    ],
    "additionalProperties": false,
    "properties": {
      "domain_name": {
        "type": "string",
        "title": "Domain name (org name part of your Okta URL)",
        "examples": [
          "dev-12345678"
        ]
      },
      "token": {
        "type": "string",
        "title": "Okta Token",
        "airbyte_secret": true
      },
      "version": {
        "type": "string",
        "title": "version",
        "default": "v1"
      }
    }
  }
}
