{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Google Calendar Spec",
    "type": "object",
    "required": [
      "client_email",
      "private_key"
    ],
    "additionalProperties": false,
    "properties": {
      "client_email": {
        "type": "string",
        "title": "Google Calendar Client Email",
        "examples": [
          "https://my-google-calendar-server.example.com"
        ]
      },
      "private_key": {
        "type": "string",
        "title": "Google Calendar Private Key",
        "airbyte_secret": true
      },
      "calendar_id": {
        "type": "string",
        "title": "Google Calendar Calendar Id",
        "default": "primary"
      },
      "events_max_results": {
        "type": "number",
        "title": "Google Calendar Events Max Results",
        "default": 2500,
        "max": 2500
      },
      "calendars_max_results": {
        "type": "number",
        "title": "Google Calendar Calendar Max Results",
        "default": 250,
        "max": 250
      }
    }
  }
}
